{
    "sap.app": {
        "id": "requirements",
        "title": "Requerimientos",
        "dataSources": {
            "AdminReqServ": {
                "uri": "/admin-req/",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": { "libs": { "sap.fe.templates": {} } },
        "models": {
            "": {
                "dataSource": "AdminReqServ",
                "settings": { 
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect" : true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
                
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": "",
                    "name": "RequirementsList",
                    "target": "RequirementsList"
                },
                {
                    "pattern": "Requirements({key})",
                    "name": "RequirementObjectPage",
                    "target": "RequirementObjectPage"
                }
            ],
            "targets": {
                "RequirementsList": {
                    "type": "Component",
                    "id": "RequirementsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings" : {
                            "entitySet" : "Requirements",
                            "navigation": {
                                "Requirements":{
                                    "detail":{
                                        "route":"RequirementsDetail"
                                    }
                                }
                            } 
                      
                             
                        }
                    }
                },
                "RequirementObjectPage": {
                    "type": "Component",
                    "id": "RequirementObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings":{
                            "entitySet": "Requirements"
                        }
                    }
                }
            }
        }
    }
}